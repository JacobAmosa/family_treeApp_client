<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="@color/grey">


        <RelativeLayout
            android:layout_width="394dp"
            android:layout_height="321dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="6dp">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="196dp"
                android:layout_height="52dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="-1dp"
                android:layout_marginTop="84dp"
                android:text="@string/story_line"
                android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

            <Spinner
                android:id="@+id/life_spinner"
                android:layout_width="130dp"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="193dp"
                android:layout_marginTop="85dp" />

            <Switch
                android:id="@+id/life_switch"
                android:layout_width="50dp"
                android:layout_height="46dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="82dp"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                android:checked="true"
                tools:ignore="UseSwitchCompatOrMaterialXml" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="189dp"
                android:layout_height="53dp"
                android:layout_alignBottom="@+id/textView7"
                android:layout_alignParentTop="true"
                android:layout_marginStart="-2dp"
                android:layout_marginTop="184dp"
                android:layout_marginBottom="0dp"
                android:text="@string/tree_lines"
                android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                tools:ignore="NotSibling" />

            <Spinner
                android:id="@+id/tree_spinner"
                android:layout_width="132dp"
                android:layout_height="52dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="190dp"
                android:layout_marginTop="182dp" />

            <Switch
                android:id="@+id/tree_switch"
                android:layout_width="50dp"
                android:layout_height="49dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="330dp"
                android:layout_marginTop="179dp"
                android:checked="true"
                tools:ignore="UseSwitchCompatOrMaterialXml" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView7"
                android:layout_alignParentTop="true"
                android:layout_marginStart="4dp"
                android:layout_marginTop="263dp"
                android:text="@string/spouse_lines"
                android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

            <Spinner
                android:id="@+id/spouse_spinner"
                android:layout_width="130dp"
                android:layout_height="53dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="194dp"
                android:layout_marginTop="256dp"
                android:layout_marginBottom="-24dp" />

            <Switch
                android:id="@+id/spouse_switch"
                android:layout_width="51dp"
                android:layout_height="53dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="327dp"
                android:layout_marginTop="256dp"
                android:layout_marginEnd="12dp"
                android:checked="true" />

        </RelativeLayout>


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="127dp"
            android:layout_marginTop="336dp"
            android:text="Map Type"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

        <Spinner
            android:id="@+id/map_spinner"
            android:layout_width="166dp"
            android:layout_height="54dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="238dp"
            android:layout_marginBottom="270dp" />

        <TextView
            android:id="@+id/resync_text"
            android:layout_width="344dp"
            android:layout_height="57dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="2dp"
            android:layout_marginBottom="116dp"
            android:text="@string/resync"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="1dp"
            android:layout_marginBottom="129dp"
            android:text="@string/resync_two" />

        <TextView
            android:id="@+id/logout_text"
            android:layout_width="363dp"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-1dp"
            android:layout_marginBottom="45dp"
            android:text="@string/myLogout"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-3dp"
            android:layout_marginBottom="54dp"
            android:text="@string/logout_two" />
    </RelativeLayout>

</FrameLayout>